{
  "version": 2,
  "builds": [
    {
      "src": "pythoncoding4u/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "pythoncoding4u/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "pythoncoding4u.settings"
  },
  "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
}
