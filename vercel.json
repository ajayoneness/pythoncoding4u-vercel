{
  "version": 2,
  "builds": [
    {
      "src": "pythoncoding4u/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "30mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    {
      "src": "/staticfiles/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "pythoncoding4u/wsgi.py"
    }
  ]
}